{
  "name": "tree-sitter-jshtml",
  "version": "0.20.0",
  "description": "HTML grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer"
  ],
  "homepage": "https://github.com/Scheduleflow/tree-sitter-jshtml",
  "repository": {
    "type": "git",
    "url": "git@github.com:Scheduleflow/tree-sitter-jshtml.git"
  },
  "authors": [
    "Vadim Pavlov <vaddsm@gmail.com>",
    "Max Brunsfeld <maxbrunsfeld@gmail.com>",
    "Ashi Krishnan <queerviolet@github.com>",
    "Amaan Qureshi <amaanq12@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "nan": "^2.18.0"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-google": "^0.14.0",
    "tree-sitter-cli": "^0.20.8"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "lint": "eslint grammar.js",
    "test": "tree-sitter test && tree-sitter parse examples/*.html --quiet --time",
    "test-windows": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "text.jshtml.basic",
      "injection-regex": "jshtml",
      "file-types": [
        "html"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "indents": [
        "queries/indents.scm"
      ],
      "textobjects": [
        "queries/textobjects.scm"
      ],
      "matchup": [
        "queries/matchup.scm"
      ],
      "injections": [
        "queries/injections.scm"
      ]
    }
  ]
}
